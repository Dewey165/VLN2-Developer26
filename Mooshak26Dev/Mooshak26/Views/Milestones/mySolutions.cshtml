@model IEnumerable<Mooshak26.Models.Entities.SubmittedSolution>
@{
    ViewBag.Title = "mySolutions";
}

<h2>User solutions to milestone</h2>
<h4>Feedback page for submitted solutions</h4>

<table class="table">
    <tr>
        <th>
            Input
        </th>
        <th>
            Expected Output
        </th>
        <th>
            User Output
        </th>
        <th>
            Correct
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.input)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.expectedOutput)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.output)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.correct)
            </td>
        </tr>
    }
</table>
@Html.ActionLink("Go Back", "Index", new { controller = "Milestones", id = Model.ElementAt(0).assignmentID }, new { @class = "btn btn-default" })
